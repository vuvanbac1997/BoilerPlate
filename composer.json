{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.6.*",
        "predis/predis": "1.*",
        "aws/aws-sdk-php": "3.12.*",
        "facebook/graph-sdk": "5.3.*",
        "abraham/twitteroauth": "0.6.*",
        "maknz/slack": "^1.7",
        "icanboogie/inflector": "^1.4",
        "laravel/socialite": "^3.0.12",
        "google/apiclient": "^2.2.2",
        "dusterio/laravel-aws-worker": "^0.1.5",
        "cviebrock/eloquent-sluggable": "^4.5.1",
        "laravel/passport": "^4.0",
        "laravel/tinker": "^1.0",
        "fideloper/proxy": "~4.0"
    },
    "require-dev": {
        "mockery/mockery": "1.1.0",
        "phpunit/phpunit": "~7.3.0",
        "fzaninotto/faker": "^1.8.0",
        "phpspec/phpspec": "~5.0.0",
        "barryvdh/laravel-debugbar": "~3.1.5",
        "barryvdh/laravel-ide-helper": "^2.1",
        "symfony/css-selector": "4.0.*",
        "symfony/dom-crawler": "4.0.*",
        "doctrine/dbal": "~2.8.0",
        "laravel/browser-kit-testing": "^4.0.0",
        "filp/whoops": "^2.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
